{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy/$1"
    }
  ],
  "functions": {
    "api/proxy/[...path].ts": {
      "maxDuration": 30
    }
  }
}
